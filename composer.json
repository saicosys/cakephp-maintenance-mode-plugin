{
    "name": "saicosys/cakephp-maintenance",
    "description": "Saicosys/Maintenance plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Saicosys",
            "homepage": "https://github.com/saicosys"
        },
        {
            "name": "Sandeep Kadyan",
            "homepage": "https://github.com/sandeep-kadyan"
        }
    ],
    "require": {
        "php": ">=8.1",
        "cakephp/cakephp": "5.1.*"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^5.0",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "Saicosys\\Maintenance\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Saicosys\\Maintenance\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
